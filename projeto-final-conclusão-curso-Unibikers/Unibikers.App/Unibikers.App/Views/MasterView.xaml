<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Unibikers.App.Views.MasterView"
             xmlns:pages="clr-namespace:Unibikers.App.Views">
  <FlyoutPage.Flyout>
    <pages:MasterViewMenu x:Name="FlyoutPage"/>
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage BackgroundColor="{StaticResource corSecundaria}">
      <x:Arguments>
        <pages:MasterViewDetail />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>